{
  "name": "@magic-rpc/server",
  "version": "1.2.4",
  "description": "Magic RPC server with GraphQL-like resolver mechanism",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c && tsc --emitDeclarationOnly",
    "dev": "rollup -c --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "npm-publish": "npm publish"
  },
  "keywords": [
    "rpc",
    "resolver",
    "dataloader",
    "graphql-like",
    "typescript",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@haiyami/hyperstruct": "^1.8.0",
    "dataloader": "^2.2.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
