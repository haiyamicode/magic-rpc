{
  "name": "magic-rpc",
  "version": "1.0.3",
  "private": true,
  "description": "Magic RPC monorepo with server and client packages",
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "clean": "turbo run clean",
    "type-check": "turbo run type-check",
    "example": "yarn workspace example dev",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "check": "biome check . --write",
    "precommit": "biome check . --write && turbo run type-check"
  },
  "keywords": [
    "rpc",
    "resolver",
    "dataloader",
    "graphql-like",
    "typescript",
    "monorepo"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "biome": "^0.3.3",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.6",
    "typescript": "^5.0.0"
  }
}
